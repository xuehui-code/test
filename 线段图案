import java.io.*;

public class Main {
   public static void main(String[] args)throws IOException{
       BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
       String s;
       while((s = br.readLine())!=null){
           int num = Integer.parseInt(s);
           int i = 0;
           for(i=0;i<num;i++){
               System.out.print("*");
           }
           System.out.println();
       }
   }
}
